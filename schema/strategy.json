{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "strategy.json",
  "title": "Strategy",
  "description": "A Yearn strategy",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "authorUri": {
      "type": "string",
      "format": "uri"
    },
    "media": {
      "type": "string",
      "format": "uri"
    }
  },
  "required": ["name", "description"]
}
